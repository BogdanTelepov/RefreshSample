<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tokenapp">

    <application
        android:name="com.example.registrationapp.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TokenApp">
        <activity android:name="com.example.registrationapp.authorization.AuthWithNumberActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name="com.example.registrationapp.SignInOrRegistrationActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name="com.example.registrationapp.registration.GetFirebaseMessage"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name="com.example.registrationapp.registration.RegisterNumber"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name="com.example.registrationapp.authorization.AuthGetMessage"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name="com.example.registrationapp.HomeActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>